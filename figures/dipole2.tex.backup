\usetikzlibrary{calc}

\begin{tikzpicture}
  
  \foreach \x in {1,...,200}
  {
    \pgfmathparse{rnd}
    \pgfmathsetmacro{\a}{2.5*\pgfmathresult + 0.15}
    \pgfmathparse{rnd}
    \pgfmathsetmacro{\b}{2.3*\pgfmathresult + 0.05}
    \pgfmathsetmacro{\c}{\b - 4/(1+3*\a)/(1+3*\a)}
        
    \ifthenelse{\lengthtest{\c pt > 0.1 pt}}{\draw[filled, color=pdcolor7] (\a, \b) circle (0.03);}{}
    \ifthenelse{\lengthtest{\c pt < - 0.1 pt}}{\draw[filled, color=pdcolor6] (\a, \b) circle (0.03);}{}
  }
  
  \draw[>=latex, <->, thick] node[left, yshift = 3cm]{\scriptsize$F(Q^2)$} ++ (0, 3) -- (0, 0) -- node[below, xshift = 1.5cm]{\scriptsize log10$(Q^2)$} ++ (3,0);
  
  \draw[smooth, ultra thick, variable=\x, domain=-0.6:0.3] plot ({\x}, {4/(1+pow(10.0,3*\x))/(1+pow(10.0,3*\x)});

    \draw[smooth, ultra thick, variable=\x, domain=0:0.3] plot ({\x}, {pow(10, \x)});

  \draw[color=pdcolor3, dashed] (0, 2.36) -- (2.7,2.36) -- (2.7, 0);

\end{tikzpicture}
